      <%= link_to "modifier", edit_tank_path(tank)  %>
      <%= link_to "supprimer cette cuve", tank_path(tank), method: :delete, data: {confirm: "Voulez-vous supprimer cette cuve ?"}  %>
    <% end %>
  </div>
  <div class="card">
    <%= link_to "ajouter une cuve", new_tank_path  %>
